  <record id="studio_customization.odoo_studio_crm_lead_ec06239a-514a-4c32-83fa-a48acad9f2da" model="ir.ui.view" context="{'studio': True}">
    <field name="active" eval="True"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//field[@name='date_deadline']" position="attributes">
          <attribute name="attrs">{"required": [["type","=","opportunity"]]}</attribute>
        </xpath>
        <xpath expr="//field[@name='application_id']" position="after">
          <xpath expr="//field[@name='user_id']" position="move"/>
          <field name="x_studio_officer_1"/>
          <field name="x_studio_support_team"/>
        </xpath>
        <xpath expr="//field[@name='contractor_id']" position="attributes">
          <attribute name="attrs">{}</attribute>
          <attribute name="invisible">1</attribute>
        </xpath>
        <xpath expr="//field[@name='team_id']" position="attributes">
          <attribute name="attrs">{}</attribute>
          <attribute name="invisible">1</attribute>
        </xpath>
        <xpath expr="//form[1]/sheet[1]/group[1]/group[@name='lead_priority']/field[@name='tag_ids']" position="attributes">
          <attribute name="attrs">{}</attribute>
          <attribute name="string">Product line</attribute>
        </xpath>
        <xpath expr="//field[@name='description']" position="attributes">
          <attribute name="attrs">{}</attribute>
          <attribute name="required">1</attribute>
